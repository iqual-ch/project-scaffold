{
  "name": "iqual/project-scaffold",
  "description": "Interactive project scaffolding and initializing.",
  "type": "composer-plugin",
  "keywords": ["scaffold", "kickstart", "init", "bootstrap", "template"],
  "homepage": "https://www.iqual.ch",
  "license": "GPL-2.0-or-later",
  "authors": [
      {
          "name": "martinstadelmann",
          "email": "m.stadelmann@iqual.ch"
      }
  ],
  "require": {
    "php": ">=7.3.0",
    "composer-plugin-api": "^2.0",
    "symfony/yaml": "^4.0 || ^5.0 || ^6.0 || ^7.0",
    "twig/twig": "^2.0 || ^3.0"
  },
  "autoload": {
    "psr-4": {
      "iqual\\Composer\\ProjectScaffold\\": "src"
    }
  },
  "extra": {
    "class": "iqual\\Composer\\ProjectScaffold\\Plugin"
  },
  "config": {
    "sort-packages": true
  },
  "require-dev": {
    "composer/composer": "^2.0"
  }
}
